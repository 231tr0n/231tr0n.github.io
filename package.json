{
	"name": "231tr0n.github.io",
	"private": true,
	"version": "1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"show-deps-updates": "npm-check-updates",
		"update-deps": "npm-check-updates -u && npm install",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --list-different --write .",
		"lint": "npm run check && prettier --check . && eslint .",
		"lint-fix": "eslint --fix . && prettier --list-different --write .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"ci": "npm run lint && npm run build && npm run test"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.39.2",
		"@html-eslint/eslint-plugin": "^0.54.2",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.2.2",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"npm-check-updates": "^19.3.2",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"rollup-plugin-visualizer": "^6.0.5",
		"svelte": "^5.50.0",
		"svelte-check": "^4.3.6",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"ace-code": "^1.43.6"
	}
}
